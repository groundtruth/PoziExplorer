            "type": "OpenLayers.Layer.TMS",
            "args": [
                "Mapscape",
                "http://map1.spatialvision.com.au/mapscape_em_vic",
                {
                    "layers" : "basic",
                    "attribution": "<img style='bottom:15px;padding-right:110px;width:80px;height:78px;z-index:1000;opacity:0.75;filter:alpha(opacity=75);' src='theme/app/img/custom/mapscape_logo.png'>",
                    "projection" : new OpenLayers.Projection("EPSG:900913"),
                    "restrictedExtent" : new OpenLayers.Bounds(16077927,-4525170,16200223,-4402205),
                    "numZoomLevels": 20,
                    "sphericalMercator" : true,
                    "getURL": function (bounds) {
                        var res = this.map.getResolution();
                        var x = Math.round((bounds.left - this.maxExtent.left) / (res * this.tileSize.w));
                        var y = Math.round((this.maxExtent.top - bounds.top) / (res * this.tileSize.h));
                        var z = this.map.getZoom();
                        return 'http://spatialvision.pozi.com/mapscape_em_vic/z'+z+'/y'+y+'/'+x+'_'+y+'_'+z+'.png';
                    }
                }
            ]
        },
